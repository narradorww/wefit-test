        "statements": 18.23,
        "branches": 10.09,
        "functions": 19.42,
        "lines": 18.24
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
    

  },

  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
